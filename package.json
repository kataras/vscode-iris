{
  "name": "iris",
  "displayName": "Iris Web Framework (Go)",
  "version": "0.0.7",
  "publisher": "kataras2006",
  "description": "Iris Web Framework snippets for Visual Studio Code - https://iris-go.com",
  "private": false,
  "author": {
    "name": "Gerasimos Maropoulos - kataras2006@hotmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kataras/vscode-iris/issues",
    "email": "kataras2006@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kataras/vscode-iris.git"
  },
  "homepage": "https://github.com/kataras/vscode-iris/blob/master/README.md",
  "icon": "images/icon.png",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters",
    "Debuggers",
    "Formatters",
    "Testing"
  ],
  "galleryBanner": {
    "color": "#F2F2F2",
    "theme": "light"
  },
  "keywords": [
    "iris",
    "go",
    "iris go",
    "golang",
    "iris golang",
    "multi-root ready"
  ],
  "extensionDependencies": [
    "golang.Go"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.5.0",
    "@types/vscode": "^1.59.0"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "activationEvents": [
    "onLanguage:go",
    "workspaceContains:*.go",
    "workspaceContains:*/*.go",
    "workspaceContains:*/*/*.go",
    "onCommand:go.show.commands",
    "onDebugResolve:go"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "go",
        "path": "./snippets/go.json"
      }
    ],
    "configurationDefaults": {
      "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
          "source.organizeImports": true
        },
        "editor.suggest.snippetsPreventQuickSuggestions": false
      }
    }
  }
}
